  </div>
  <div class="field">
    <%= f.label :team_b %><br />
    <%= f.number_field :team_b %>
  </div>
  <div class="field">
    <%= f.label :public %><br />
    <%= f.check_box :public %>
  </div>
  <div class="field">
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :auto_assign %><br />
    <%= f.check_box :auto_assign %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
