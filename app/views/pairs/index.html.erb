<h1>Listing pairs</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Answer</th>
    <th>Choice a</th>
    <th>Choice b</th>
    <th>Choice c</th>
    <th>Choice d</th>
    <th>Sheet</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pairs.each do |pair| %>
  <tr>
    <td><%= pair.question %></td>
    <td><%= pair.answer %></td>
    <td><%= pair.choice_a %></td>
    <td><%= pair.choice_b %></td>
    <td><%= pair.choice_c %></td>
    <td><%= pair.choice_d %></td>
    <td><%= pair.sheet_id %></td>
    <td><%= link_to 'Show', pair %></td>
    <td><%= link_to 'Edit', edit_pair_path(pair) %></td>
    <td><%= link_to 'Destroy', pair, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pair', new_pair_path %>
